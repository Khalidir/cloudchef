

<div class="container">
  <div class="row">
    <div class="col-sm-4 col-sm-offset-3">
      <h3>Review your meal</h3>
      <%= simple_form_for [@meal, @review] do |f| %>
        <%= f.input :rating %>
        <%= f.input :content %>
        <%= f.submit "Submit", class: "btn btn-outline-dark" %> <%= link_to "Back to meals", meals_path, class: "btn btn-outline-dark" %>
      <% end %>
    </div>
  </div>
</div>
